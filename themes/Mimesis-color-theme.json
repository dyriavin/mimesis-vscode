  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment",
        "storage.type.class.jsdoc"
      ],
      "settings": {
        "foreground": "#454545",
        "background": "#FFFBF2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#293338"
      }
    },
    {
      "scope": [
        "constant",
        "markup.bold",
        "markup.italic",
        "variable.other",
        "support.constant",
        "variable.language",
        "variable.parameter",
        "entity.name.constant",
        "entity.name.namespace",
        "variable.other.constant",
        "variable.other.enummember",
        "storage.type.function.arrow",
        "punctuation.definition.arrow",
        "constant.other.placeholder.go",
        "punctuation.definition.string",
        "support.type.property-name.css",
        "string punctuation.section.embedded source",
        "variable.parameter.function.language.python"
      ],
      "settings": {
        "foreground": "#454545"
      }
    },
    {
      "scope": [
        "keyword.other.unit",
        "keyword.control.java",
        "keyword.control.else",
        "keyword.control.shell",
        "keyword.other.DML.sql",
        "keyword.other.DDL.create.II.sql",
        "keyword.control.break",
        "keyword.control.while",
        "keyword.operator.class",
        "keyword.control.elseif",
        "keyword.control.continue",

        "keyword.control.exception",
        "entity.name.type.parameter",
        "keyword.operator.decrement",
        "keyword.operator.increment",
        "keyword.operator.comparison",

        "keyword.operator.arithmetic",
        "keyword.control.conditional",
        "keyword.operator.increment-decrement",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css"
      ],
      "settings": {
        "foreground": "#b5007f"
      }
    },
    {
      "scope": [
        "storage",
        "keyword",
        "storage.type",
        "meta.selector.css",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0060DF"
      }
    },
    {
      "scope": ["entity.name.tag", "entity.name.tag.css"],
      "settings": {
        "foreground": "#78458c"
      }
    },
    {
      "scope": [
        "storage.type.java",
        "storage.modifier.import",
        "storage.modifier.package",
        "variable.parameter.function"
      ],
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": ["meta.property-name"],
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "scope": [
        "markup.heading",
        "constant.other.caps.python",
        "markup.heading entity.name",
        "string.regexp constant.character.escape"
      ],
      "settings": {
        "foreground": "#058B00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "support",
        "variable",
        "markup.quote",
        "markup.inserted",
        "keyword.key.toml",
        "support.function.go",
        "entity.name.function",
        "entity.name.type.cpp",
        "entity.name.tag.yaml",
        "entity.name.import.go",
        "entity.name.function.go",
        "meta.diff.header.to-file",
        "string.quoted.double.html",
        "entity.name.tag.localname",
        "support.type.property-name",
        "entity.name.function.python",
        "meta.function.decorator.python",
        "entity.name.function.member.cpp",
        "support.function.builtin.python"
      ],
      "settings": {
        "foreground": "#006504"
      }
    },
    {
      "scope": [
        "invalid.broken",
        "invalid.illegal",
        "invalid.deprecated",
        "invalid.unimplemented"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#EA4334"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#fafbfc"
      }
    },
    {
      "scope": ["message.error", "brackethighlighter.unmatched"],
      "settings": {
        "foreground": "#EA4334"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#4285F4"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp",
        "string.regexp.character-class",
        "string.regexp source.ruby.embedded",
        "string.regexp constant.character.escape",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#363959"
      }
    },
    {
      "scope": [
        "constant.language.go",
        "constant.language.null",
        "constant.language.python",
        "constant.language.boolean",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#D70022",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "meta.output",
        "meta.diff.header",
        "markup.inline.raw",
        "meta.module-reference"
      ],
      "settings": {
        "foreground": "#0060df"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#EA1537"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#f6f8fa"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#a35ebf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0060df"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.angle",
        "brackethighlighter.quote",
        "brackethighlighter.square"
      ],
      "settings": {
        "foreground": "#586069"
      }
    },
    {
      "scope": [
        "string.other.link",
        "constant.other.reference.link",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#b5007f",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Braces and Operators",
      "scope": [
        "punctuation.definition.class.begin.bracket.curly",
        "punctuation.definition.class.end.bracket.curly",
        "punctuation.definition.begin.bracket.curly",
        "punctuation.definition.end.bracket.curly",
        "punctuation.definition.begin.bracket.square",
        "punctuation.definition.end.bracket.square",
        "punctuation.section.array.end",
        "punctuation.section.array.begin",
        "meta.brace.square",
        "meta.brace.round",
        "punctuation.definition.block",
        "keyword.operator.assignment",
        "punctuation.definition.arrow",
        "keyword.operator.key.php",
        "keyword.operator.null-coalescing.php",
        "keyword.operator.string.php"
      ],
      "settings": {
        "foreground": "#b5007f"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type.class",
        "support.other.namespace",
        "entity.name.type.namespace",
        "support.class",
        "constant.other.php",
        "entity.other.alias.php",
        "entity.other.inherited-class.php"
      ],
      "settings": {
        "foreground": "#D70022"
      }
    },
    {
      "name": "Class properties",
      "scope": ["variable.other.property.php"],
      "settings": {
        "foreground": "#7d004f"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "meta.class.body.php constant.other.php",
        "meta.class.php constant",
        "constant.other.class.php"
      ],
      "settings": {
        "foreground": "#006504",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "meta.function-call.php entity.name.function.php",
        "meta.method-call.static.php meta.method-call.php entity.name.function",
        "meta.class.body.php meta.method-call.static.php entity.name.function"
      ],
      "settings": {
        "foreground": "#006504"
      }
    },
    {
      "name": "Functions type hinting",
      "scope": [
        "keyword meta.function.parameter.typehinted.php",
        "keyword.other.type.php"
      ],
      "settings": {
        "foreground": "#D70022"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable.other.php"],
      "settings": {
        "foreground": "#293338"
      }
    },
    {
      "name": "PHP",
      "scope": "punctuation.section.embedded.begin.php",
      "settings": {
        "foreground": "#0060DF"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric.decimal", "constant.numeric.hex"],
      "settings": {
        "foreground": "#283636"
      }
    },
    {
      "name": "Meta attributes",
      "scope": ["support.attribute.builtin.php", "meta.attribute.php"],
      "settings": {
        "foreground": "#008EA4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP static call",
      "scope": "meta.method-call.static.php entity.name.function",
      "settings": {
        "foreground": "#363959",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#000F40",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Delimiters",
      "scope": ["punctuation.separator.delimiter"],
      "settings": {
        "foreground": "#2A3434"
      }
    },
    {
      "name": "Named Attribute",
      "scope": ["entity.name.variable.parameter.php"],
      "settings": {
        "foreground": "#535e65",
        "fontStyle": "underline"
      }
    }
  ]
}
